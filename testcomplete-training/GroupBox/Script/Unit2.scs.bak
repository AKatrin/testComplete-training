Given("I display the UI-elemtes tab", function (){
  var tabControl = rxMainFrame["RxTabcontrol"];
  tabControl["ClickTab"]("UI-element test area");
});

When("I select the green color", function (){
  var groupBox = tabControl["RxTabUIElements"]["grpBox"]["grpBoxRadioButton"];
  groupBox["rdbGreenLight"]["ClickButton"]();
});

Then("I see the squere green", function (){
  aqObject["CheckProperty"](Aliases["RxDemoApp"]["RxMainFrame"]["RxTabcontrol"]["RxTabUIElements"]["grpBox"]["grpBoxRadioButton"]["pnlColourPanel"], "PropBackColor", cmpEqual, 5);
  aqObject["CheckProperty"](Aliases["RxDemoApp"]["RxMainFrame"]["RxTabcontrol"]["RxTabUIElements"]["grpBox"]["grpBoxRadioButton"]["pnlColourPanel"], "PropForeColor", cmpEqual, 6);
  aqObject["CheckProperty"](Aliases["RxDemoApp"]["RxMainFrame"]["RxTabcontrol"]["RxTabUIElements"]["grpBox"]["grpBoxRadioButton"]["pnlColourPanel"], "WndCaption", cmpEqual, "");
});
